{
    "products": [
        {
            "name": "multi-cloud-demo",
            "label": "Multi Cloud Demo",
            "product_kind": "solution",
            "tags": ["network"],
            "keywords": ["azure", "website", "static", "cos"],
            "short_description": "This is a demonstration of similar terraform templates using different cloud providers.",
            "long_description": "This example is designed to illustrate how similar terraform templates work across two differnt cloud platforms.",
            "offering_docs_url": "https://raw.githubusercontent.com/vburckhardt/multicloud-static-website/main/README.md",
            "offering_icon_url": "data:image/svg+xml;base64,",
            "support_details": "This is a demonstration only and no official support is available.",
            "flavors": [
                {
                    "label": "IBM Cloud",
                    "name": "ibm-cloud-example",
                    "install_type": "fullstack",
                    "working_directory": "ibm",
                    "usage_template": "module \"ibm-cos-website-demo\" {\n  source           = \"https://cm.globalcatalog.cloud.ibm.com/api/v1-beta/offering/source//${{workingDirectory}}?archive=tgz&catalogID=${{catalogID}}&flavor=${{flavor}}&kind=${{kind}}&name=${{name}}&version=${{version}}\"\n  resource_group   = var.resource_group\n  \n  prefix           = var.prefix\n  region           = var.region\n  }",
                    "compliance": {
                        "authority": "scc-v3",
                        "controls": [
                            {
                                "profile": {
                                    "name": "IBM Cloud for Financial Services",
                                    "version": "1.2.0"
                                },
                                "names": [
                                    "CM-7(a)"
                                ]
                            }
                        ]
                    },
                    "configuration": [
                        {
                            "key": "resource_group"
                        },
                        {
                            "key": "ibmcloud_api_key",
                            "required": true,
                            "type": "password"
                        },
                        {
                            "key": "prefix",
                            "required": true,
                            "type": "string"
                        },
                        {
                            "custom_config": {
                                "config_constraints": {
                                    "generationType": "2"
                                },
                                "grouping": "deployment",
                                "original_grouping": "deployment",
                                "type": "vpc_region"
                            },
                            "key": "region",
                            "required": true
                        }
                    ],
                    "iam_permissions": [
                        {
                            "role_crns": [
                                "crn:v1:bluemix:public:iam::::serviceRole:Manager"
                            ],
                            "service_name": "cloud-object-storage"
                        }
                    ],
                    "architecture": {
                        "features": [
                            {
                                "description": "IBM",
                                "title": "Cloud Provider"
                            },
                            {
                                "description": "IBM Cloud Object Storage service",
                                "title": "Storage"
                            }
                        ],
                        "diagrams": [
                            {
                                "diagram": {
                                    "caption": "IBM Cloud",
                                    "url": "https://raw.githubusercontent.com/vburckhardt/multicloud-static-website/main/images/ibm-arch-diagram.svg"
                                },
                                "description": "This illustrates the use of the IBM Cloud and the IBM Cloud Object Storage service to host a webiste static web page."
                            }
                        ]
                    }
                },
                {
                    "label": "Azure",
                    "name": "azure-cloud-example",
                    "install_type": "fullstack",
                    "working_directory": "azure",
                    "configuration": [
                        {
                            "key": "client_id"
                        },
                        {
                            "key": "client_secret",
                            "required": true,
                            "type": "password"
                        },
                        {
                            "key": "tenant_id",
                            "required": true
                        },
                        {
                            "key": "subscription_id",
                            "required": true
                        },
                        {
                            "key": "prefix",
                            "type": "string"
                        }
                    ],
                    "iam_permissions": [
                    ],
                    "architecture": {
                        "features": [
                            {
                                "title": "Cloud Provider",
                                "description": "Azure"
                            }
                        ],
                        "diagrams": [
                            {
                                "diagram": {
                                    "caption": "Azure Cloud",
                                    "url": "https://raw.githubusercontent.com/vburckhardt/multicloud-static-website/main/images/azure-arch-diagram.svg"
                                },
                                "description": "Place holder description."
                            }
                        ]
                    }
                },
                {
                    "label": "AWS",
                    "name": "aws-cloud-example",
                    "install_type": "fullstack",
                    "working_directory": "aws",
                    "configuration": [
                        {
                            "key": "access_key"
                        },
                        {
                            "key": "secret_key",
                            "required": true,
                            "type": "password"
                        },
                        {
                            "key": "region"
                        },
                        {
                            "key": "prefix",
                            "type": "string"
                        }
                    ],
                    "iam_permissions": [
                    ],
                    "architecture": {
                        "features": [
                            {
                                "title": "Cloud Provider",
                                "description": "AWS"
                            }
                        ],
                        "diagrams": [
                            {
                                "diagram": {
                                    "caption": "AWS Cloud",
                                    "url": "https://raw.githubusercontent.com/vburckhardt/multicloud-static-website/main/images/aws-arch-diagram.svg"
                                },
                                "description": "Place holder description."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
